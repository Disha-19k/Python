    "def recur_sum(n):\n",
    "    if(n<=1):\n",
    "        return n\n",
    "    else:\n",
    "        return n+recur_sum(n-1)\n",
    "num=int(input(\"enter the number\"))\n",
    "if(num<0):\n",
    "    print(\"positive numbwe\")\n",
    "else:\n",
    "    print(\"the sum is\",recur_sum(num))"
